enum Color {
    Red,
    Green,
    Blue,
}

enum Shape {
    Circle(int),
    Rectangle(int, int),
    Point,
}

fn describe_shape(s: Shape) -> int {
    match s {
        Shape::Circle(r) => r,
        Shape::Rectangle(w, h) => w + h,
        Shape::Point => 0,
    }
}

let s1: Shape = Shape::Circle(5);
let s2: Shape = Shape::Rectangle(3, 4);
let s3: Shape = Shape::Point;

print(describe_shape(s1));
print(describe_shape(s2));
print(describe_shape(s3));
